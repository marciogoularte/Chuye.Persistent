<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <appSettings>
    <add key="NHibernate:alwaysCommit" value="false" />
  </appSettings>
  <connectionStrings>
    <add name="PubsMysql" connectionString="server=ubuntu-16;database=pubs;user id=root;pwd=123456" providerName="MySql.Data.MySqlClient" />
    <add name="PubsMongo" connectionString="mongodb://ubuntu-16/Pubs" />
  </connectionStrings>
  
<system.data>
    <DbProviderFactories>
      <remove invariant="MySql.Data.MySqlClient" />
      <add name="MySQL Data Provider" invariant="MySql.Data.MySqlClient" description=".Net Framework Data Provider for MySQL" type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data, Version=6.9.8.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d" />
    </DbProviderFactories>
  </system.data></configuration>
